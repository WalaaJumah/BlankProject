<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestSuite" thread-count="2" parallel="classes" >
    <listeners>
        <listener class-name="retry_failed_test_cases.AnnotationTransfer"/>
        <listener class-name="extent_report_listner.ExtentReportListener" />
    </listeners>

    <test name="ChromeTest" parallel="methods">
        <parameter name="environment" value="https://sporter.com" />

        <parameter name="browser" value="Edge" />
        <!--        <parameter name="browser" value="chrome" />-->
        <classes>
            <class name="sporter_test.homepage_classes.QatarHomePageTestCases"></class>

        </classes>
    </test>

    <test name="Edge" parallel="methods">
        <parameter name="environment" value="https://www.sporter.com" />

        <parameter name="browser" value="Edge" />
        <!--        <parameter name="browser" value="chrome" />-->
        <classes>
            <class name="sporter_test.AeProductDetailsPageTestCases"></class>

        </classes>
    </test>

</suite>


